/*
 * This is a dummy class
*/


class Test {

    integer i;
    float f;
    
    // actor
    Test(integer ii, float ff) {
	i = ii;
	f = ff;
    }

    // calculate the minimum between two integers
    integer min(integer x, integer y) {
	if (x < y) {
	    return x;
	} else if (x > y) {
	    return y;
	} else if (x == y) {
	    return x;
	}
    }

    // calculate the maximum between two floats
    float max(float x, float y) {
        if (x <= y) {
	    return y;
	} else if (x >= y) {
	    return x;
	}
    }

    // compare if two integers are equal
    int notEqual(integer x, integer y) {
	if (x <> y) return 1;
	return 0;
    }

    // plus two integers by using stupid and complex way
    int plus(integer x, integer y) {
        int n = x;
	for (integer i = 0; i < y; ) {
	    n = n + 2;
	    n = n - 1;
	    n = n * 1;
	    n = n / 1;
	    i = i + 1;
	}
        return n;
    }

    // calculate the sum of an integer array
    int sum() {
	integer[3] array = {1, 2, 3};
        integer sum = 0;
	for (integer i : array) {
	    sum = sum + i;
	}
	return sum;
    }

    main() {
        int x = 0;
	int y = 0;
	float z = 1.0;
	Test t;
        if (0 == x && 0 == y || !(0.0 == z)) {
            read();
	} else {
	    write();
	}
    }
}

